<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog title="{i18n>viewGeralAvisoAguardarUpload}" showHeader="true" type="Message" class="sapUiNoContentPadding">
		<content>
			<Table items="{ path: 'anexos>/' }" width="400px">
				<columns>
					<Column></Column>
					<Column></Column>
				</columns>
				<items>
					<ColumnListItem>
						<cells>
							<ObjectIdentifier title="{anexos>nome_arquivo}"/>
							<ProgressIndicator percentValue="{anexos>progresso}" displayValue="{anexos>progresso}%" showValue="true" state="Information"/>
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Dialog>
</core:FragmentDefinition>