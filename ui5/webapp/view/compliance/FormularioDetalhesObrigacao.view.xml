<mvc:View
	controllerName="ui5ns.ui5.controller.compliance.FormularioDetalhesObrigacao"
	xmlns:mvc="sap.ui.core.mvc" 
	xmlns:core="sap.ui.core"
	xmlns:l="sap.ui.layout" 
	xmlns:f="sap.ui.layout.form"
	xmlns:f2="sap.f"
	xmlns="sap.m" 
	class="viewComplianceFormularioDetalhesObrigacao">
	<f2:DynamicPage title="{i18n>viewComplianceFormularioDetalhesObrigacaoTituloPagina}" showFooter="true">
		<f2:title>
			<f2:DynamicPageTitle>
				<f2:breadcrumbs>
					<Breadcrumbs>
						<Link text="Home" enabled="false" />
						<Link text="{i18n>ViewFormularioListagemObrigacoes}" enabled="false" />
						<Link text="{i18n>viewFormularioComplianceFormularioDetalheObrigacao}" enabled="false" />
					</Breadcrumbs>
				</f2:breadcrumbs>
				<f2:heading>
					<Title text="{i18n>viewFormularioComplianceComplianceFormulariodeDetalhedeObrigacao}"/>
				</f2:heading>
			</f2:DynamicPageTitle>
		</f2:title>
		<f2:header>
			<f2:DynamicPageHeader pinnable="false">
				<f2:content>
					<FlexBox alignItems="Start" justifyContent="SpaceBetween" class="sapUiSmallMarginBottom">
						<items>
							<Panel backgroundDesign="Transparent" class="sapUiNoContentPadding">
								<content>
									<l:VerticalLayout>
										<l:HorizontalLayout allowWrapping="true" class="sapUiSmallMarginBottom">
											<!--<ObjectIdentifier title="{/RespostaObrigacao/id_resposta_obrigacao}" />-->
										</l:HorizontalLayout>
										<!--<l:HorizontalLayout allowWrapping="true">
											<l:VerticalLayout>
												<ObjectAttribute title="Ano Calendário" text="2018"/>
											</l:VerticalLayout>
										</l:HorizontalLayout>-->
									</l:VerticalLayout>
								</content>
							</Panel>
						</items>
					</FlexBox>
				</f2:content>
			</f2:DynamicPageHeader>
		</f2:header>
		<f2:content>
			<VBox class="sapUiSmallMargin">
				<f:Form id="formularioObrigacao" 
					editable="true">
					<f:title>
						<!--<core:Title text="{i18n>formularioObrigacaoTituloObrigacao}"/>-->
						<core:Title text="{/RespostaObrigacao/nome_obrigacao}"/>
					</f:title>
					<f:layout>
						<f:ResponsiveGridLayout labelSpanXL="4" labelSpanL="4" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0"
							emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="2" columnsL="2" columnsM="1" singleContainerFullSize="false"/>
					</f:layout>
					<f:formContainers>
						<f:FormContainer>
							<f:formElements>
								<f:FormElement label="{i18n>ViewGeralDataInício}">
									<f:fields>
										<DatePicker 
												placeholder="{i18n>viewGeralInsiraUmaData}" 
												valueFormat="yyyy-MM-dd" 
												value="{/RespostaObrigacao/data_inicial}" 
												enabled="false"
												/>
										<!--<Label text="{/RespostaObrigacao/ano_calendario}"/>-->
									</f:fields>
								</f:FormElement>
								<f:FormElement label="{i18n>ViewGeralDataFim}">
									<f:fields>
										<DatePicker 
												placeholder="{i18n>viewGeralInsiraUmaData}" 
												valueFormat="yyyy-MM-dd" 
												value="{/RespostaObrigacao/data_final}" 
												enabled="false"
												/>
										<!--<Label text="{/RespostaObrigacao/ano_calendario}"/>-->
									</f:fields>
								</f:FormElement>
								<f:FormElement label="{i18n>formularioObrigacaoLabelPrazoEntrega}">
									<f:fields>
										<DatePicker 
												placeholder="{i18n>viewGeralInsiraUmaData}" 
												valueFormat="yyyy-MM-dd" 
												value="{/RespostaObrigacao/prazo_entrega}" 
												enabled="false"
												/>
										<!--<Label text="{/RespostaObrigacao/ano_calendario}"/>-->
									</f:fields>
								</f:FormElement>
								<f:FormElement label="{i18n>formularioObrigacaoLabelPais}">
									<f:fields>
											<Input value="{/RespostaObrigacao/pais}" enabled="false" />
									</f:fields>
								</f:FormElement>
								<f:FormElement label="{i18n>formularioObrigacaoLabelExtensao}">
									<f:fields>
											<DatePicker 
												placeholder="{i18n>viewGeralInsiraUmaData}" 
												valueFormat="yyyy-MM-dd" 
												value="{/RespostaObrigacao/data_extensao}" 
												enabled="{= !!${/RespostaObrigacao/ind_extensao_compliance} === true ? (!${/JaEstavaPreenchido} ? true : false ) : false}"/>
									</f:fields>
								</f:FormElement>
								<f:FormElement label="{i18n>viewComplianceListagemObrigacoesColunaPeriodicidade}">
									<f:fields>
											<Input enabled="false" value="{/RespostaObrigacao/descricao}"></Input>
									</f:fields>
								</f:FormElement>
								<f:FormElement label="{i18n>ViewGeralSuporteHired}">
									<f:fields>
										<CheckBox id="checkSuporteContratado" select="onTrocarSuporte" selected="{/RespostaObrigacao/suporte_contratado}" />
									</f:fields>
								</f:FormElement>
								<f:FormElement label="{i18n>formularioObrigacaoLabelSuporte}">
									<f:fields>
											<TextArea 
											value="{/RespostaObrigacao/suporte_especificacao}"
											id="textAreaSuporte"
											rows="5" 
											enabled="{= ${/RespostaObrigacao/suporte_contratado} ? true : false}"/>
									</f:fields>
								</f:FormElement>
								<f:FormElement label="{i18n>formularioObrigacaoLabelValorSuporte}">
									<f:fields>
										<Input id="ValorSuporte" value="{/RespostaObrigacao/suporte_valor}"  type="Number" enabled="{= ${/RespostaObrigacao/suporte_contratado} ? true : false}"/>
									</f:fields>
								</f:FormElement>
								<f:FormElement label="{i18n>viewGeralMoeda}">
									<f:fields>
										
										<!--<Select 
												id="selectTipoMoeda" 
												items="{/DominioMoeda}" 
												selectedKey="{/RespostaObrigacao/fk_id_dominio_moeda.id_dominio_moeda}">
												<core:Item text="{nome}" key="{id_dominio_moeda}" />
											</Select>-->
											<ComboBox
											items="{/DominioMoeda}"
											selectedKey="{/RespostaObrigacao/fk_id_dominio_moeda.id_dominio_moeda}"
											enabled="{= ${/RespostaObrigacao/suporte_contratado} ? true : false}"
											>
											<core:Item text="{nome}" key="{id_dominio_moeda}" />
											</ComboBox>
									</f:fields>
								</f:FormElement>
								<f:FormElement label="{i18n>viewGeralEmpresa}">
									<f:fields>
												<!--<Select 
												id="selectTipoEmpresa" 
												items="{/Empresa}" 
												selectedKey="{/RespostaObrigacao/fk_id_empresa.id_empresa}">
												<core:Item text="{nome}" key="{id_empresa}" />
												</Select>-->
												<Input id="Empresa" value="{/RespostaObrigacao/nome}" enabled="false" />
									</f:fields>
								</f:FormElement>
								<f:FormElement label="{i18n>formularioObrigacaoLabelAnoFiscal}">
									<f:fields>
											<DatePicker id="datepickerAnoFiscal" placeholder="Insira uma data" valueFormat="yyyy-MM-dd" value="{/RespostaObrigacao/ano_fiscal}" enabled="false" />
											<!--<Label text="{/RespostaObrigacao/ano_calendario}"/>-->
									</f:fields>
								</f:FormElement>
								<f:FormElement label="{i18n>viewComplianceListagemObrigacoesSelectAnoCalendario}">
									<f:fields>
										<DatePicker 
												id="datepickerAnoCalendario" 
												placeholder="Insira uma data..." 
												valueFormat="yyyy-MM-dd" 
												value="{/RespostaObrigacao/ano_calendario}" 
												enabled="false"
												/>
										<!--<Label text="{/RespostaObrigacao/ano_calendario}"/>-->
									</f:fields>
								</f:FormElement>
							</f:formElements>
						</f:FormContainer>
					</f:formContainers>
				</f:Form>
				<f:Form id="formularioDocumentos">
					<f:title>
						<core:Title text="{i18n>formularioObrigacaoTituloDocumentos}"/>
					</f:title>
					<f:layout>
						<f:ResponsiveGridLayout labelSpanXL="4" labelSpanL="4" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0"
							emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="2" columnsL="2" columnsM="1" singleContainerFullSize="true"/>
					</f:layout>
					<f:formContainers>
						<f:FormContainer>
							<f:formElements>
								<f:FormElement>
									<UploadCollection
										id="UploadCollection"
										maximumFilenameLength="{settings>/maximumFilenameLength}"
										maximumFileSize="{settings>/maximumFileSize}"
										multiple="true"
										change="onChange"
										fileDeleted="onFileDeleted"
										filenameLengthExceed="onFilenameLengthExceed"
										fileRenamed="onFileRenamed"
										fileSizeExceed="onFileSizeExceed"
										fileType="{fileTypes>/selected}"
										selectionChange="onSelectionChange"
										typeMissmatch="onTypeMissmatch"
										uploadComplete="onUploadComplete"
										beforeUploadStarts="onBeforeUploadStarts"
										uploadTerminated="onUploadTerminated"
										numberOfAttachmentsText="Uploaded (8)"
										showSeparators="{settings>/showSeparators}"
										uploadEnabled="{settings>/uploadEnabled}"
										uploadButtonInvisible="{=!${settings>/uploadButtonVisible}}"
										mode="{settings>/mode}">
										<!--items="{
											path: '/items',
											templateShareable: false
										}"-->
										<toolbar>
											<OverflowToolbar id="myId">
												<Title id="attachmentTitle" />
												<ToolbarSpacer />
												<SearchField width="10rem" search="onSearch"
											placeholder="{i18n>ViewGeralProcurar}" tooltip= "{i18n>ViewGeralProcurar}" />
												<Button
													id="deleteSelectedButton"
													text="Delete"
													press="onDeleteSelectedItems"
													enabled="false"
													visible="false"
													type="Transparent" />
												<ToggleButton
													id="selectAllButton"
													text="Select all"
													press="onSelectAllPress"
													enabled="false"
													visible="false"
													type="Transparent" />
												<UploadCollectionToolbarPlaceholder />
											</OverflowToolbar>
										</toolbar>
										<!--<items>
											<UploadCollectionItem
												documentId="{documentId}"
												fileName="{fileName}"
												mimeType="{mimeType}"
												thumbnailUrl="{thumbnailUrl}"
												url="{url}"
												enableEdit="{settings>/enableEdit}"
												enableDelete="{settings>/enableDelete}"
												visibleDelete="{settings>/visibleDelete}"
												visibleEdit="{settings>/visibleEdit}"
												attributes="{path : 'attributes', templateShareable : false}"
												statuses="{path : 'statuses', templateShareable : false}"
												markers="{path : 'markers', factory: '.createObjectMarker'}"
												selected="{selected}">
												<attributes>
													<ObjectAttribute title="{title}"
														text="{path : 'text', formatter : '.formatAttribute'}"
														active="{active}"
														press="onAttributePress" />
												</attributes>
												<statuses>
													<ObjectStatus title="{title}"
														text="{text}"
														state="{state}" />
												</statuses>
											</UploadCollectionItem>
										</items>-->
									</UploadCollection>
								</f:FormElement>
							</f:formElements>
						</f:FormContainer>
					</f:formContainers>
				</f:Form>
			</VBox>
		</f2:content>
		<f2:footer>
			<Toolbar>
				<ToolbarSpacer/>
				<Button 
					text="{i18n>formularioObrigacaoBotaoSalvar}" 
					type="Accept" 
					press="onSalvar"/>
				<Button 
					text="{i18n>formularioObrigacaoBotaoCancelar}" 
					type="Reject" 
					press="onCancelar"/>
			</Toolbar>
		</f2:footer>
	</f2:DynamicPage>	
</mvc:View>