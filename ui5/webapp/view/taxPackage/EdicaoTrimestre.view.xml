									<t:Column width="12rem" hAlign="End">
										<Label text="{i18n>viewEdiçãoTrimestreCurrentYearCredit}" textAlign="Center" width="100%"/>
										<t:template>
											<Text text="{path: 'current_year_value', type: 'sap.ui.model.type.Float', formatOptions: {minFractionDigits: 2, maxFractionDigits:2}}" />
										</t:template>
									</t:Column>
									<t:Column width="12rem" hAlign="End">
										<Label text="{i18n>viewEdiçãoTrimestreCurrentYearCreditU}" textAlign="Center" width="100%"/>
										<t:template>
											<Text text="{path: 'current_year_value_utilized', type: 'sap.ui.model.type.Float', formatOptions: {minFractionDigits: 2, maxFractionDigits:2}}" />
										</t:template>
									</t:Column>
									<t:Column width="12rem" hAlign="Center">
										<Label text="{i18n>viewEdiçãoTrimestreAdjustments}" textAlign="Center" width="100%"/>
										<t:template>
											<Input textAlign="End" value="{path: 'adjustments', type: 'sap.ui.model.type.Float', formatOptions: {minFractionDigits: 2, maxFractionDigits:2}}" change="onAplicarRegras" enabled="true" /> <!-- enabled="{= ${ind_corrente} === true ? true : false}"  -->
										</t:template>
									</t:Column>
									<t:Column width="15rem">
										<Label text="{i18n>viewEdiçãoTrimestreJustificativa}" textAlign="Center" width="100%"/>
										<t:template>
											<Input type="Text" value="{justificativa}" enabled="{justificativaEnabled}" valueState="{justificativaValueState}" valueStateText="{i18n>viewGeralCampoNaoPodeSerVazio}" change="onTrocarJustificativa" />
										</t:template>
									</t:Column>
									<t:Column width="13rem" hAlign="End">
										<Label text="{i18n>viewEdiçãoTrimestreCurrentYearCreditE}" textAlign="Center" width="100%"/>
										<t:template>
											<Text text="{path: 'current_year_value_expired', type: 'sap.ui.model.type.Float', formatOptions: {minFractionDigits: 2, maxFractionDigits:2}}" />
										</t:template>
									</t:Column>
									<t:Column width="12rem" hAlign="End">
										<Label text="{i18n>viewEdiçãoTrimestreClosingBalance}" textAlign="Center" width="100%"/>
										<t:template>
											<Text text="{path: 'closing_balance', type: 'sap.ui.model.type.Float', formatOptions: {minFractionDigits: 2, maxFractionDigits:2}}" />
										</t:template>
									</t:Column>
									<t:Column width="15rem">
										<Label text="{i18n>viewEdiçãoTrimestreOBS}" textAlign="Center" width="100%"/>
										<t:template>
											<Input value="{obs}" enabled="true" /> <!-- enabled="{= ${ind_corrente} === true ? true : false}" -->
										</t:template>
									</t:Column>
								</t:columns>
								<t:footer>
									<VBox>
										<Text text="Total" />
										<HBox>
											<Text text="{i18n>viewTaxPackageOpeningBalance}" width="12rem" />
											<!--<Text text="999.999.999,99" textAlign="End" width="7rem" />-->
											<Text text="{path: '/TotalCreditSchedule/opening_balance', type: 'sap.ui.model.type.Float', formatOptions: {minFractionDigits: 2, maxFractionDigits:2}}" textAlign="End" width="9rem" />
										</HBox>
										<HBox>
											<Text text="{i18n>viewTaxPackageCurrentYearLoss}" width="12rem" />
											<Text text="{path: '/TotalCreditSchedule/current_year_value', type: 'sap.ui.model.type.Float', formatOptions: {minFractionDigits: 2, maxFractionDigits:2}}" textAlign="End" width="9rem" />
										</HBox>
										<HBox>
											<Text text="{i18n>viewTaxPackageCurrentYearLossUtilized}" width="12rem" />
											<Text text="{path: '/TotalCreditSchedule/current_year_value_utilized', type: 'sap.ui.model.type.Float', formatOptions: {minFractionDigits: 2, maxFractionDigits:2}}" textAlign="End" width="9rem" />
										</HBox>
										<HBox>
											<Text text="{i18n>viewTaxPackageAdjustments}" width="12rem" />
											<Text text="{path: '/TotalCreditSchedule/adjustments', type: 'sap.ui.model.type.Float', formatOptions: {minFractionDigits: 2, maxFractionDigits:2}}" textAlign="End" width="9rem" />
										</HBox>
										<HBox>
											<Text text="{i18n>viewTaxPackageCurrentYearLossExpired}" width="12rem" />
											<Text text="{path: '/TotalCreditSchedule/current_year_value_expired', type: 'sap.ui.model.type.Float', formatOptions: {minFractionDigits: 2, maxFractionDigits:2}}" textAlign="End" width="9rem" />
										</HBox>
										<HBox>
											<Text text="{i18n>viewTaxPackageClosingBalance}" width="12rem" />
											<Text text="{path: '/TotalCreditSchedule/closing_balance', type: 'sap.ui.model.type.Float', formatOptions: {minFractionDigits: 2, maxFractionDigits:2}}" textAlign="End" width="9rem" />
										</HBox>
									</VBox>
								</t:footer>
							</t:Table>
						</ScrollContainer>
					</IconTabFilter>
				</items>
			</IconTabBar>
		</f2:content>
		<f2:footer>
			<Toolbar>
				<ToolbarSpacer/>
				<Button id="btnSalvarFechar" text="{i18n>viewGeralSalvarFechar}" type="Accept" press="onSalvarFechar" />
				<Button id="btnSalvar" text="{i18n>formularioObrigacaoBotaoSalvar}" type="Accept" press="onSalvar" />
				<Button id="btnCancelar" text="{i18n>formularioObrigacaoBotaoCancelar}" type="Reject" press="onCancelar" />
			</Toolbar>
		</f2:footer>
	</f2:DynamicPage>
</mvc:View>