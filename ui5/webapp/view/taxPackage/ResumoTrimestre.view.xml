									<Column
										hAlign="Center">
										<Text text="{i18n>viewTAXResumoTrimestreColunaTaxableIncome}" />
									</Column>
									<Column
										hAlign="Center">
										<Text text="{i18n>viewTAXResumoTrimestreColunaNetLocalTax}" />
									</Column>
									<Column
										hAlign="Center">
										<Text text="{i18n>viewTAXResumoTrimestreColunaTaxDue}" />
									</Column>
								</columns>
								<items>
									<ColumnListItem>
										<cells>
											<ObjectNumber
												number="{primeiroValor}" />
											<ObjectNumber
												number="{segundoValor}" />
											<ObjectNumber
												number="{terceiroValor}" />
											<ObjectNumber
